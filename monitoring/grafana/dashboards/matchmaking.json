{
  "title": "Matchmaking Overview",
  "uid": "matchmaking-overview",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "tags": ["matchmaking"],
  "editable": true,
  "panels": [
    {
      "type": "stat",
      "title": "New Enqueues (5m)",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(players_enqueued_new_total)",
          "legendFormat": "new (total)"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Requeues (5m)",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(players_requeued_total)",
          "legendFormat": "requeue (total)"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Allocated (success) (5m)",
      "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(players_allocated_total)",
          "legendFormat": "allocated (total)"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Queue Wait Quantiles",
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum by (le) (rate(match_wait_duration_seconds_bucket[5m])))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.9, sum by (le) (rate(match_wait_duration_seconds_bucket[5m])))",
          "legendFormat": "p90"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(match_wait_duration_seconds_bucket[5m])))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },
    {
      "type": "timeseries",
      "title": "Abnormal Behaviors (rate)",
      "gridPos": { "x": 12, "y": 4, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "sum(rate(abnormal_unknown_type_total[5m]))",
          "legendFormat": "UnknownType"
        },
        {
          "expr": "sum(rate(abnormal_missing_field_total[5m]))",
          "legendFormat": "MissingField"
        },
        {
          "expr": "sum(rate(abnormal_duplicate_enqueue_total[5m]))",
          "legendFormat": "DuplicateEnqueue"
        },
        {
          "expr": "sum(rate(abnormal_wrong_session_id_total[5m]))",
          "legendFormat": "WrongSessionId"
        },
        {
          "expr": "sum(rate(loading_session_timeout_players_total[5m]))",
          "legendFormat": "TimeoutPlayers"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Players in Queue by Mode",
      "gridPos": { "x": 0, "y": 10, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "sum by (game_mode) (players_in_queue_by_mode)",
          "legendFormat": "{{game_mode}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Match Time Quantiles",
      "gridPos": { "x": 12, "y": 10, "w": 12, "h": 6 },
      "targets": [
        { "expr": "histogram_quantile(0.5, sum by (le) (rate(match_time_seconds_bucket[5m])))", "legendFormat": "p50" },
        { "expr": "histogram_quantile(0.9, sum by (le) (rate(match_time_seconds_bucket[5m])))", "legendFormat": "p90" },
        { "expr": "histogram_quantile(0.99, sum by (le) (rate(match_time_seconds_bucket[5m])))", "legendFormat": "p99" }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    },
    {
      "type": "timeseries",
      "title": "Loading Duration Quantiles",
      "gridPos": { "x": 0, "y": 16, "w": 12, "h": 6 },
      "targets": [
        { "expr": "histogram_quantile(0.5, sum by (le) (rate(loading_duration_seconds_bucket[5m])))", "legendFormat": "p50" },
        { "expr": "histogram_quantile(0.9, sum by (le) (rate(loading_duration_seconds_bucket[5m])))", "legendFormat": "p90" },
        { "expr": "histogram_quantile(0.99, sum by (le) (rate(loading_duration_seconds_bucket[5m])))", "legendFormat": "p99" }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    },
    {
      "type": "stat",
      "title": "State Violations (total)",
      "gridPos": { "x": 0, "y": 22, "w": 6, "h": 4 },
      "targets": [
        { "expr": "sum(state_violations_total)", "legendFormat": "violations" }
      ]
    },
    {
      "type": "stat",
      "title": "HTTP Timeouts (total)",
      "gridPos": { "x": 6, "y": 22, "w": 6, "h": 4 },
      "targets": [
        { "expr": "sum(http_timeout_errors_total)", "legendFormat": "http timeouts" }
      ]
    },
    {
      "type": "stat",
      "title": "Redis Conn Failures (total)",
      "gridPos": { "x": 12, "y": 22, "w": 6, "h": 4 },
      "targets": [
        { "expr": "sum(redis_connection_failures_total)", "legendFormat": "redis failures" }
      ]
    },
    {
      "type": "timeseries",
      "title": "By-Mode Counters (rate)",
      "gridPos": { "x": 12, "y": 16, "w": 12, "h": 6 },
      "targets": [
        { "expr": "sum by (game_mode) (rate(enqueued_total_by_mode[5m]))", "legendFormat": "enqueued {{game_mode}}" },
        { "expr": "sum by (game_mode) (rate(matched_players_total_by_mode[5m]))", "legendFormat": "matched {{game_mode}}" },
        { "expr": "sum by (game_mode) (rate(loading_completed_total_by_mode[5m]))", "legendFormat": "loading_done {{game_mode}}" },
        { "expr": "sum by (game_mode) (rate(dedicated_allocation_success_total_by_mode[5m]))", "legendFormat": "alloc_ok {{game_mode}}" }
      ]
    },
    {
      "type": "stat",
      "title": "TimeoutPlayers (15m increase)",
      "gridPos": { "x": 18, "y": 22, "w": 6, "h": 4 },
      "targets": [
        { "expr": "sum(increase(loading_session_timeout_players_total[15m]))", "legendFormat": "timeouts (15m)" }
      ]
    }
  ]
}
