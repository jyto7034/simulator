{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "title": "Test Behavior",
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": { "refresh_intervals": ["2s", "5s", "10s", "30s", "1m", "5m"] },
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "templating": {
    "list": [
      {
        "name": "run_id",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(test_player_behavior_total, run_id)",
        "refresh": 1,
        "multi": true,
        "includeAll": true,
        "current": { "selected": false, "text": "All", "value": ["$__all"] }
      },
      {
        "name": "game_mode",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(test_player_behavior_total, game_mode)",
        "refresh": 1,
        "multi": true,
        "includeAll": true,
        "current": { "selected": false, "text": "All", "value": ["$__all"] }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Total Test Behaviors",
      "datasource": "Prometheus",
      "targets": [
        { "expr": "sum(test_player_behavior_total) or vector(0)", "refId": "A" }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Slow Loader (5m)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(increase(test_player_behavior_total{kind=\"slow_loader\"}[5m])) or vector(0)",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 }
    },

    {
      "id": 3,
      "type": "timeseries",
      "title": "Behavior rate by kind",
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "targets": [
        {
          "expr": "sum by (kind) (rate(test_player_behavior_total{run_id=~\"$run_id\", game_mode=~\"$game_mode\"}[$__rate_interval]))",
          "legendFormat": "{{kind}}",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 7 }
    },

    {
      "id": 4,
      "type": "timeseries",
      "title": "Behavior rate by kind/game_mode",
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "targets": [
        {
          "expr": "sum by (kind, game_mode) (rate(test_player_behavior_total{run_id=~\"$run_id\", game_mode=~\"$game_mode\"}[$__rate_interval]))",
          "legendFormat": "{{kind}}/{{game_mode}}",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 12, "y": 4, "w": 12, "h": 7 }
    },

    {
      "id": 5,
      "type": "table",
      "title": "Top run_id (15m)",
      "datasource": "Prometheus",
      "options": { "showHeader": true },
      "targets": [
        {
          "expr": "topk(10, sum by (run_id) (increase(test_player_behavior_total{game_mode=~\"$game_mode\"}[15m])))",
          "format": "table",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 11, "w": 12, "h": 7 }
    },

    {
      "id": 6,
      "type": "stat",
      "title": "Invalid Message (5m)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(increase(test_player_behavior_total{kind=\"invalid_message\"}[5m])) or vector(0)",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 12, "y": 11, "w": 6, "h": 4 }
    },
    {
      "id": 7,
      "type": "stat",
      "title": "Wrong Session Id (5m)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(increase(test_player_behavior_total{kind=\"wrong_session_id\"}[5m])) or vector(0)",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 18, "y": 11, "w": 6, "h": 4 }
    }
  ]
}

